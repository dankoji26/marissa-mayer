<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Reservation.
    -->
    <changeSet id="20230531072906-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="reservation"
                                 constraintName="fk_reservation__user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="utilisateur"/>

        <addForeignKeyConstraint baseColumnNames="cours_id"
                                 baseTableName="reservation"
                                 constraintName="fk_reservation__cours_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cours"/>
    </changeSet>
</databaseChangeLog>
