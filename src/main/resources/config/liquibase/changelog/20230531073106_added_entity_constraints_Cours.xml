<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Cours.
    -->
    <changeSet id="20230531073106-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="cours_id"
                                 baseTableName="rel_cours__categories"
                                 constraintName="fk_rel_cours__categories__cours_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cours"/>

        <addForeignKeyConstraint baseColumnNames="categories_id"
                                 baseTableName="rel_cours__categories"
                                 constraintName="fk_rel_cours__categories__categories_id"
                                 referencedColumnNames="id"
                                 referencedTableName="categorie"/>

        <addForeignKeyConstraint baseColumnNames="catalogue_id"
                                 baseTableName="cours"
                                 constraintName="fk_cours__catalogue_id"
                                 referencedColumnNames="id"
                                 referencedTableName="catalogue"/>
    </changeSet>
</databaseChangeLog>
